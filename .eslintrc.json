{
    "extends": "airbnb-base",
    "env": {
        "mocha": true,
        "node": true,
        "browser": true
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off",
        "no-shadow": "off",
        "no-unused-expressions": "off",
        "prefer-destructuring": "off",
        "no-restricted-globals": "off",
        "consistent-return": "off",
        "no-undef": "off"
    }
}